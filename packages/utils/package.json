{
  "name": "@adonisjs/utils",
  "version": "1.3.0",
  "description": "A collection of utilities to keep AdonisJs codebase dry",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "keywords": [
    "adonisjs",
    "utils"
  ],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/utils"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
