{
  "name": "@adonisjs/response",
  "version": "1.0.0",
  "description": "Nodejs http response builder",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "prepublishOnly": "npm run build",
