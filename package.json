{
  "name": "adonis-fold",
  "version": "2.0.0",
  "description": "Dependency manager and ioc container for your next NodeJs application",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run standard && istanbul cover _mocha --report lcovonly -- -R spec test/unit && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "istanbul cover _mocha test/unit --bail",
    "standard": "standard src/**/*.js test/**/*.spec.js"
  },
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "co": "^4.6.0",
    "cat-log": "^1.0.0",
    "co-parallel": "^1.0.0",
    "dwell": "^1.0.0",
    "lodash": "^3.10.1",
    "require-stack": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/adonis-fold.git"
  },
  "keywords": [
    "ioc",
    "dependency",
    "injection",
    "service",
    "provider",
    "autoloading"
  ],
  "bugs": {
    "url": "https://github.com/poppinss/adonis-fold/issues"
  },
  "homepage": "https://github.com/poppinss/adonis-fold#readme"
}
